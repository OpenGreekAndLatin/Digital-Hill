                   <lb/><s n="2"><w n="1">θάρσει</w><w n="2">,</w> <w n="3">μέγας</w> <w n="4">σοι</w> <w n="5">τοῦδ᾽</w> <w n="6">ἐγὼ</w> <w n="7">φόβου</w> <w n="8">μοχλός</w><w n="9">.</w></s>
            	</quote>
            	<s n="3"><w n="1">κἀν</w> <w n="2">ἄλλοις</w> <w n="3">δὲ</w> <w n="4">τὴν</w> <w n="5">ἄγκυραν</w> <w n="6">ἰσχάδα</w> <w n="7">κέκληκεν</w> <w n="8">διὰ</w> <w n="9">τὸ</w> <w n="10">κατέχειν</w> <w n="11">τὴν</w> <w n="12">ναῦν</w></s>
