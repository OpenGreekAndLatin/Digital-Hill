	<body>
        <div type="textpart" subtype="chapter" n="46">
            <p>
           		<s n="1"><w n="1">Χαμαιλέων</w> <w n="2">δ᾽</w> <w n="3">ὁ</w> <w n="4">Ποντικὸς</w> <w n="5">ἐν</w> <w n="6">τῷ</w> <w n="7">περὶ</w> <w n="8">Ἀνακρέοντος</w> <w n="9">προθεὶς</w> <w n="10">τὸ</w></s>
				<quote rend="blockquote">
                  	<s n="1"><lb/><w n="11">ξανθῇ</w> <w n="12">δ᾽</w> <w n="13">Εὐρυπύλῃ</w> <w n="14">μέλει</w>
                  		<lb/><w n="15">ὁ</w> <w n="16">περιφόρητος</w> <w n="17">Ἀρτέμων</w><w n="18">,</w></s>
                </quote>
                <s n="1"><w n="19">τὴν</w> <w n="20">προσηγορίαν</w> <w n="21">ταύτην</w> <w n="22">λαβεῖν</w> <w n="23">τὸν</w> <w n="24">Ἀρτέμωνα</w>
              	<w n="25">διὰ</w> <w n="26">τὸ</w> <w n="27">τρυφερῶς</w> <w n="28">βιοῦντα</w> <w n="29">περιφέρεσθαι</w> <w n="30">ἐπὶ</w> <w n="31">κλίνης</w><w n="32">.</w></s> <s n="2"><w n="1">καὶ</w> <w n="2">γὰρ</w> 
               	<w n="3">Ἀνακρέων</w> <w n="4">αὐτὸν</w><w n="5">,</w></s> κ.τ.λ.
