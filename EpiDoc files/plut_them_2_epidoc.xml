	<p>
	    <s n="1"><w n="1">καίτοι</w> <w n="2">Στησίμβροτος</w> <w n="3">Ἀναξαγόρου</w> <w n="4">τε</w> <w n="5">διακοῦσαι</w> <w n="6">τὸν</w> <w n="7">Θεμιστοκλέα</w> <w n="8">φησὶ</w> <w n="9">καὶ</w> <w n="10">περὶ</w> <w n="11">Μέλισσον</w> <w n="12">σπουδάσαι</w> <w n="13">τὸν</w> <w n="14">φυσικόν</w><w n="15">,</w> <w n="16">οὐκ</w> <w n="17">εὖ</w> <w n="18">τῶν</w> <w n="19">χρόνων</w> <w n="20">ἁπτόμενος</w><w n="21">·</w></s> 
